{
  "compilerOptions": {
    "preserveConstEnums": true,
    "strict": true,
    "target": "ESNext",
    "module": "ESNext",
    "rootDir": "./src",
    "outDir": "./dist",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "downlevelIteration": true,
    "resolveJsonModule": true,
    "skipLibCheck": true
  }
}
